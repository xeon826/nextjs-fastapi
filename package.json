{
  "name": "nextjs-fastapi",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "generate-types": "openapi-typescript http://localhost:8000/api/py/openapi.json -o types/generated.ts",
    "fastapi-dev": "poetry run python -m uvicorn api.index:app --reload",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "python-install": "poetry install",
    "python-update": "poetry update",
    "migrate": "poetry run alembic upgrade head",
    "_comment": "Example usage: npm run migrate-create -- \"initial tables\"",
    "migrate-create": "poetry run alembic revision --autogenerate -m", 
    "migrate-downgrade": "poetry run alembic downgrade -1",
    "stop-backend": "pkill -f \"uvicorn api.index:app\" || true"
  },
  "dependencies": {
    "better-auth": "^1.3.34",
    "@daveyplate/better-auth-ui": "^3.2.10",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "next": "^15.1.3",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.7.2"
  }
}
